{
  "name": "wasteman-client",
  "version": "3.1.0",
  "private": true,
  "dependencies": {
    "@aspnet/signalr": "^1.0.3",
    "bootstrap": "3.4.1",
    "chartist": "0.10.1",
    "chartist-plugin-pointlabels": "0.0.6",
    "chartist-plugin-tooltips-updated": "0.0.20",
    "moment": "^2.22.2",
    "node-sass": "4.6.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "4.1.2",
    "react": "16.3.0",
    "react-bootstrap": "0.32.1",
    "react-chartist": "0.13.1",
    "react-datepicker": "^1.6.0",
    "react-dom": "16.3.0",
    "react-gauge-animated": "^0.1.9",
    "react-google-maps": "9.4.5",
    "react-notification-system": "0.2.17",
    "react-redux": "^5.0.7",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.1.1",
    "react-spinjs-fix": "^3.1.0",
    "react-switch": "^3.0.4",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "build-env": "env.sh && move env.js .\\public",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p build-env watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
